{
  "name": "devops-firefighter-backend",
  "version": "1.0.0",
  "description": "Autonomous AI Incident Response System - Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
