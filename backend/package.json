    "e2b": "^2.3.4",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "axios": "^1.6.7",
    "zod": "^3.22.4",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
